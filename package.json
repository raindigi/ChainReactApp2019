{
  "name": "chain-react-conf",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "android": "react-native run-android",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:setup": "touch .env && echo \"AWS_APPSYNC_API_KEY=\"$AWS_APPSYNC_API_KEY >> .env",
    "ci:test": "yarn test --ci",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "compile": "tsc --noEmit -p . --pretty",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p .",
    "format": "npm-run-all format:*",
    "ios": "react-native run-ios",
    "lint:ts": "tslint -p .",
    "lint": "npm-run-all lint:*",
    "postinstall": "node_modules/.bin/jetify && test -n \"$NOYARNPOSTINSTALL\" || solidarity",
    "precommit": "lint-staged",
    "prepare": "patch-package",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 9001 --skip-packager",
    "test": "NODE_ENV=test TZ=America/Los_Angeles jest",
    "test:coverage": "yarn test --coverage",
    "test:e2e": "detox test -c ios.sim.debug"
  },
  "dependencies": {
    "@mapbox/react-native-mapbox-gl": "https://github.com/nitaliano/react-native-mapbox-gl#master",
    "@react-native-community/async-storage": "^1.5.0",
    "@react-native-community/netinfo": "^3.2.1",
    "apisauce": "^1.0.3",
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "appcenter": "1.13.0",
    "appcenter-analytics": "1.13.0",
    "appcenter-crashes": "1.13.0",
    "appcenter-push": "1.13.0",
    "aws-amplify": "^1.1.29",
    "aws-appsync": "^1.8.1",
    "aws-appsync-react": "^1.2.9",
    "date-fns": "1.30.1",
    "date-fns-timezone": "^0.1.4",
    "graphql": "^14.4.1",
    "graphql-tag": "^2.10.1",
    "i18n-js": "^3.3.0",
    "lottie-react-native": "2.6.1",
    "mobx": "^4.15.4",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^3.10.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "16.8.6",
    "react-apollo": "^2.5.8",
    "react-native": "0.60.3",
    "react-native-device-info": "^2.1.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-hyperlink": "^0.0.14",
    "react-native-keychain": "^3.1.3",
    "react-native-localize": "^1.1.3",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-native-splash-screen": "^3.2.0",
    "react-navigation": "^3.11.0",
    "reactotron-core-client": "^2.8.3",
    "reactotron-mst": "^3.1.3",
    "reactotron-react-native": "^3.1.2",
    "uuid": "^3.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/runtime": "^7.5.4",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/react-native": "^5.1.9",
    "@storybook/theming": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/ramda": "^0.26.12",
    "@types/react": "^16.8.22",
    "@types/react-native": "^0.57.65",
    "@types/react-navigation": "^3.0.7",
    "@types/react-test-renderer": "^16.8.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "detox": "^13.1.1",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "jetifier": "^1.6.1",
    "lint-staged": "^7.0.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "npm-run-all": "^4.1.2",
    "patch-package": "^6.0.0-12",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.18.2",
    "react-dom": "^16.2.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-powerplug": "^1.0.0",
    "react-test-renderer": "16.8.6",
    "solidarity": "^2.1.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^3.5.2"
  },
  "resolutions": {
    "apollo-cache-inmemory": "^1.4.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp|ttf|otf)$": "RelativeImageStub",
      "^React$": "<rootDir>/node_modules/react"
    },
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/test/setup.ts",
      "./node_modules/appcenter/test/AppCenterMock.js",
      "./node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "./node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-native|react-navigation|@react-navigation|@storybook|@react-native-community)"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ChainReactConf.app",
        "build": "xcodebuild -workspace ios/ChainReactConf.xcworkspace -scheme ChainReactConf -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "name": "iPhone X, iOS 12.2"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/ChainReactConf.app",
        "build": "xcodebuild -workspace ios/ChainReactConf.xcworkspace -scheme ChainReactConf -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "name": "iPhone X -- iOS 12.2"
      }
    }
  }
}
